{
    "name": "kikwik/api-admin-bundle",
    "description": "Admin client for api-platform endpoint based on Hydra Core Vocabulary",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Davide Scarpato",
            "email": "davide@kikwik.it"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/http-client": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/twig-bundle": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Kikwik\\ApiAdminBundle\\": "src/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4",
        "nyholm/symfony-bundle-test": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Kikwik\\ApiAdminBundle\\Tests\\": "tests/"
        }
    }
}
